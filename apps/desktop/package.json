{
  "name": "silentsort-desktop",
  "version": "1.0.0",
  "main": "dist/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "npm run compile:electron && concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "react-scripts start",
    "dev:electron": "wait-on tcp:3000 && electron . --disable-gpu --disable-hardware-acceleration --disable-software-rasterizer",
    "compile:electron": "tsc src/main.ts --outDir dist --target es2020 --module commonjs --esModuleInterop --skipLibCheck && tsc src/preload.ts --outDir dist --target es2020 --module commonjs --esModuleInterop --skipLibCheck && tsc src/services/ai-service.ts --outDir dist/services --target es2020 --module commonjs --esModuleInterop --skipLibCheck",
    "build": "react-scripts build && npm run compile:electron",
    "dist": "npm run build && electron-builder",
    "test": "react-scripts test"
  },
  "keywords": [
    "electron",
    "ai",
    "file-organization",
    "macos"
  ],
  "author": "pranjal",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@langchain/openai": "^0.5.16",
    "chokidar": "^4.0.1",
    "dotenv": "^17.0.0",
    "electron": "^32.2.7",
    "langchain": "^0.3.29",
    "openai": "^5.8.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.1",
    "concurrently": "^9.2.0",
    "electron-builder": "^25.1.8",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "wait-on": "^8.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
